{
  "git.autofetch": true,
  "eslint.codeActionsOnSave.rules": null,
  "editor.formatOnSave": true,

  "json.schemas": [],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],

  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  }
}
